<div class="modal fade" id="importUsersModal" role="dialog" aria-labelledby="importUsersModal" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Import Users from Identity Sources</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="container">

                    

                    <div class="form-group row">
                        <label for="idp-select" class="col-sm-3 col-form-label">Source</label>
                        <select class="form-control idp-select" name="idp-select" id="idp-select" style=""></select>

                    </div>


                    <div class="form-group row">
                        <label for="user-search-select" class="col-sm-3 col-form-label">User</label>
                        <select class="form-control user-search-select" name="user-search-select" id="user-search-select" style=""></select>
                    </div>

                    <div class="form-group row">


                        <div class="form-check">
                            <label class="form-check-label">
                                <input class="form-check-input" type="checkbox" value="" id="mergeCheckbox">
                                Merge selected user with an existing user?
                            </label>
                        </div>

                    </div>



                    <div class="form-group row existingUserSelect" style="display:none;">
                        <label for="user-search-select" class="col-sm-3 col-form-label">User</label>
                        <select class="form-control existingUserSelect" name="existingUserSelect" id="existingUserSelect" style=""></select>
                    </div>



                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                <button type="button" class="btn btn-primary" id="importUserButton" onclick="AuthenticablePrincipal.ImportSelectedUsers()" data-dismiss="modal">Import Selected</button>
            </div>
        </div>
    </div>
</div>